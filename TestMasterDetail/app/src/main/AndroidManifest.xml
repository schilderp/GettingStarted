<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.lewisu.cs.howardcy.testmasterdetail" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TaskListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TaskDetailActivity"
            android:label="@string/title_task_detail"
            android:parentActivityName=".TaskListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TaskListActivity" />
        </activity>

        <provider
            android:name=".TaskProvider"
            android:authorities="edu.lewisu.cs.howardcy.TaskProvider"
            android:enabled="true"
            android:exported="false" >
        </provider>
    </application>

</manifest>
